# Smart Money PNL Reports - Cache Configuration
# Copy these settings to your main .env file

# Token Price Cache Settings
CACHE_TOKEN_SIZE=10000        # Maximum number of token prices to cache
CACHE_TOKEN_TTL=3600          # Token cache TTL in seconds (1 hour)

# Report Cache Settings  
CACHE_REPORT_SIZE=1000        # Maximum number of reports to cache
CACHE_REPORT_TTL=3600         # Report cache TTL in seconds (1 hour)

# Performance Settings
CACHE_ENABLE_METRICS=true     # Enable cache performance metrics
CACHE_ENABLE_COMPRESSION=false # Enable cache compression (future feature)

# Production Tuning Examples:
# High-frequency trading: CACHE_TOKEN_TTL=300 (5 minutes)
# Low-memory environment: CACHE_REPORT_SIZE=100, CACHE_TOKEN_SIZE=1000
# High-volume production: CACHE_REPORT_SIZE=5000, CACHE_TOKEN_SIZE=50000